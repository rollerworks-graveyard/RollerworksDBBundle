{
    "name": "rollerworks/db-bundle",
    "description": "Doctrine DBAL Helper Bundle. Providing extra functionality for the DBAL",
    "keywords": ["doctrine", "symfony", "dbal", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.0",
        "doctrine/dbal": ">=2.1"
    },
    "autoload": {
        "psr-0": {
            "Rollerworks\\Bundle\\DBBundle": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
